<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote fetch="https://github.com/openembedded" name="oe"/>
  <remote fetch="https://github.com/yoctoproject" name="yocto"/>
  <remote fetch="git://git.openembedded.org/" name="oe-core"/>

  <remote fetch="https://git.yoctoproject.org" name="meta-raspberrypi"/>
  <!-- updater RAUC -->
  <remote fetch="https://github.com/rauc" name="meta-rauc"/>
  <remote fetch="https://github.com/rauc" name="meta-raspberrypi-rauc"/>
  <!-- updater Update hub -->
  <remote fetch="https://github.com/rust-embedded" name="meta-rust-embedded"/>
  <remote fetch="https://github.com/UpdateHub" name="meta-update-hub"/>
  <remote fetch="https://github.com/UpdateHub" name="meta-update-hub-raspberrypi"/>

  <default revision="main" sync-j="4"/>

  <project name="meta-openembedded" path="sources/meta-openembedded" remote="oe" revision="scarthgap" clone-depth="1"/>
  <project name="poky" path="sources/poky" remote="yocto" revision="scarthgap" clone-depth="1"/>
  <project name="openembedded-core" path="sources/poky" remote="oe-core" revision="scarthgap" clone-depth="1"/>

  <project name="meta-raspberrypi" path="sources/meta-raspberrypi" remote="meta-raspberrypi" revision="scarthgap" clone-depth="1"/>
