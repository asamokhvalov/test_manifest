<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Basic -->  
  <remote fetch="https://github.com/openembedded" name="oe"/>
  <remote fetch="https://github.com/yoctoproject" name="poky"/>
  <!-- hw specific -->
  <remote fetch="git://git.yoctoproject.org" name="meta-raspberrypi"/>

  <default revision="main" sync-j="4"/>

  <!-- Basic -->  
  <project name="meta-openembedded" path="sources/meta-openembedded" remote="oe" revision="scarthgap" clone-depth="1"/>
  <project name="poky" path="sources/poky" remote="poky" revision="scarthgap" clone-depth="1"/>
  <!-- hw specific -->
  <project name="meta-raspberrypi" path="sources/meta-raspberrypi" remote="meta-raspberrypi" revision="scarthgap" clone-depth="1"/>
</manifest>
